{
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run development",
        "development": "cross-env NODE_ENV=development webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run development -- --watch",
        "watch-poll": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run watch -- --watch-poll",
        "hot": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run development -- --hot",
        "prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run production",
        "production": "cross-env NODE_ENV=production webpack --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.26.3",
        "axios": "^0.21.2",
        "bootstrap": "^4.0.0",
        "browser-sync": "^3.0.3",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "^7.0.3",
        "jquery": "^3.2",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "webpack": "^5.97.1",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^3.11.3"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.0",
        "@xyflow/react": "^12.3.5",
        "admin-lte": "^3.0.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sweetalert2": "^9.10.12"
    }
}
